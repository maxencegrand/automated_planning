( DEFINE ( DOMAIN SOKOBAN-TEMPORAL )
( :REQUIREMENTS :CONDITIONAL-EFFECTS :TYPING )
( :TYPES
	LOCATION - OBJECT
	T1 - OBJECT
	DIRECTION - OBJECT
	STONE - T1
	PLAYER - T1
)
( :PREDICATES
	( AT ?T10 - T1 ?LOCATION1 - LOCATION )
	( CLEAR ?LOCATION0 - LOCATION )
	( MOVE-DIR ?LOCATION0 - LOCATION ?LOCATION1 - LOCATION ?DIRECTION2 - DIRECTION )
)
( :ACTION MOVE
  :PARAMETERS ( ?PLAYER0 - PLAYER ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION ?DIRECTION3 - DIRECTION )
  :PRECONDITION
	( AND
		( CLEAR ?LOCATION2 )
		( AT ?PLAYER0 ?LOCATION1 )
		( NOT ( AT ?PLAYER0 ?LOCATION2 ) )
		( MOVE-DIR ?LOCATION1 ?LOCATION2 ?DIRECTION3 )
	)
  :EFFECT
	( AND
		( CLEAR ?LOCATION1 )
		( AT ?PLAYER0 ?LOCATION2 )
		( NOT ( CLEAR ?LOCATION2 ) )
		( NOT ( AT ?PLAYER0 ?LOCATION1 ) )
	)
)
( :ACTION PUSH
  :PARAMETERS ( ?PLAYER0 - PLAYER ?STONE1 - STONE ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION ?LOCATION4 - LOCATION ?DIRECTION5 - DIRECTION )
  :PRECONDITION
	( AND
		( NOT ( AT ?STONE1 ?LOCATION4 ) )
		( AT ?STONE1 ?LOCATION3 )
		( NOT ( CLEAR ?LOCATION2 ) )
		( CLEAR ?LOCATION4 )
		( NOT ( AT ?PLAYER0 ?LOCATION3 ) )
		( AT ?PLAYER0 ?LOCATION2 )
		( MOVE-DIR ?LOCATION3 ?LOCATION4 ?DIRECTION5 )
		( MOVE-DIR ?LOCATION2 ?LOCATION3 ?DIRECTION5 )
	)
  :EFFECT
	( AND
		( AT ?STONE1 ?LOCATION4 )
		( CLEAR ?LOCATION2 )
		( AT ?PLAYER0 ?LOCATION3 )
		( NOT ( AT ?STONE1 ?LOCATION3 ) )
		( NOT ( CLEAR ?LOCATION4 ) )
		( NOT ( AT ?PLAYER0 ?LOCATION2 ) )
	)
)
)
